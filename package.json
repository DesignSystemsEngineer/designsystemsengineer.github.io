{
  "name": "blog.designsystem.engineer",
  "description": "The blog to track progress of building DesignSystem.engineer",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "deploy": "npm run export && gh-pages -d __sapper__/export/",
    "fix:check": "prettier --check './src/**/*.{js,svelte,css,html,json}'",
    "fix:write": "prettier --write './src/**/*.{js,svelte,css,html,json}'"
  },
  "dependencies": {
    "@dsengineer/svelte": "^1.3.0",
    "compression": "^1.7.1",
    "humdrum-svelte": "^0.2.0",
    "humdrum-ui": "^1.0.3",
    "polka": "next",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.0.0",
    "@mheedev/rollup-plugin-markdown": "0.0.6",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "gh-pages": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.1",
    "postcss-import": "^12.0.1",
    "prettier": "^2.1.2",
    "prettier-plugin-svelte": "^1.4.0",
    "rollup": "^2.28.2",
    "rollup-plugin-glob": "^1.0.2",
    "rollup-plugin-svelte": "^6.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.10",
    "svelte": "^3.29.0",
    "svelte-preprocess": "^4.4.2"
  }
}
